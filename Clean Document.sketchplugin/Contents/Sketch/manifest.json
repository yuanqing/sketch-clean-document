{
  "name": "Clean Document",
  "description": "A Sketch plugin to automagically organise and clean up your Sketch document",
  "author": "Lim Yuan Qing",
  "homepage": "https://github.com/yuanqing/sketch-clean-document",
  "version": "0.0.2",
  "identifier": "yuanqing.sketch-clean-document",
  "appcast": "https://raw.githubusercontent.com/yuanqing/sketch-clean-document/master/appcast.xml",
  "bundleVersion": 1,
  "disableCocoaScriptPreprocessor": true,
  "commands": [
    {
      "name": "Clean Document",
      "identifier": "yuanqing.sketch-clean-document.clean-document",
      "script": "plugin.js",
      "handler": "clean-document"
    },
    {
      "name": "Clean Layers",
      "identifier": "yuanqing.sketch-clean-document.clean-layers.clean-layers",
      "script": "plugin.js",
      "handler": "clean-layers/clean-layers"
    },
    {
      "name": "Clean Styles and Symbols",
      "identifier": "yuanqing.sketch-clean-document.clean-styles-and-symbols.clean-styles-and-symbols",
      "script": "plugin.js",
      "handler": "clean-styles-and-symbols/clean-styles-and-symbols"
    },
    {
      "name": "Settings for Clean Layers",
      "identifier": "yuanqing.sketch-clean-document.clean-layers.settings",
      "script": "plugin.js",
      "handler": "clean-layers/settings"
    },
    {
      "name": "Settings for Clean Styles and Symbols",
      "identifier": "yuanqing.sketch-clean-document.clean-styles-and-symbols.settings",
      "script": "plugin.js",
      "handler": "clean-styles-and-symbols/settings"
    },
    {
      "name": "Reset Settings",
      "identifier": "yuanqing.sketch-clean-document.reset-settings",
      "script": "plugin.js",
      "handler": "reset-settings"
    }
  ],
  "menu": {
    "title": "Clean Document",
    "items": [
      "yuanqing.sketch-clean-document.clean-document",
      "-",
      "yuanqing.sketch-clean-document.clean-layers.clean-layers",
      "yuanqing.sketch-clean-document.clean-styles-and-symbols.clean-styles-and-symbols",
      "-",
      {
        "title": "Settings",
        "items": [
          "yuanqing.sketch-clean-document.clean-layers.settings",
          "yuanqing.sketch-clean-document.clean-styles-and-symbols.settings",
          "-",
          "yuanqing.sketch-clean-document.reset-settings"
        ]
      }
    ]
  }
}
