{
  "name": "Clean Document",
  "description": "A Sketch plugin to automagically organise and clean up your Sketch document",
  "author": "Lim Yuan Qing",
  "homepage": "https://github.com/yuanqing/sketch-clean-document",
  "version": "0.0.6",
  "identifier": "yuanqing.sketch-clean-document",
  "appcast": "https://raw.githubusercontent.com/yuanqing/sketch-clean-document/master/appcast.xml",
  "bundleVersion": 1,
  "disableCocoaScriptPreprocessor": true,
  "commands": [
    {
      "name": "Clean Document",
      "identifier": "yuanqing.sketch-clean-document.clean-document",
      "script": "plugin.js",
      "handler": "clean-document"
    },
    {
      "name": "Clean Layers",
      "identifier": "yuanqing.sketch-clean-document.clean-layers.clean-layers",
      "script": "plugin.js",
      "handler": "clean-layers/clean-layers"
    },
    {
      "name": "Clean Symbols",
      "identifier": "yuanqing.sketch-clean-document.clean-symbols.clean-symbols",
      "script": "plugin.js",
      "handler": "clean-symbols/clean-symbols"
    },
    {
      "name": "Clean Styles",
      "identifier": "yuanqing.sketch-clean-document.clean-styles.clean-styles",
      "script": "plugin.js",
      "handler": "clean-styles/clean-styles"
    },
    {
      "name": "Clean Pages",
      "identifier": "yuanqing.sketch-clean-document.clean-pages.clean-pages",
      "script": "plugin.js",
      "handler": "clean-pages/clean-pages"
    },
    {
      "name": "Settings",
      "identifier": "yuanqing.sketch-clean-document.settings.settings",
      "script": "plugin.js",
      "handler": "settings/settings"
    },
    {
      "name": "Reset Settings",
      "identifier": "yuanqing.sketch-clean-document.settings.reset-settings",
      "script": "plugin.js",
      "handler": "settings/reset-settings"
    }
  ],
  "menu": {
    "title": "Clean Document",
    "items": [
      "yuanqing.sketch-clean-document.clean-document",
      "-",
      "yuanqing.sketch-clean-document.clean-layers.clean-layers",
      "yuanqing.sketch-clean-document.clean-symbols.clean-symbols",
      "yuanqing.sketch-clean-document.clean-styles.clean-styles",
      "yuanqing.sketch-clean-document.clean-pages.clean-pages",
      "-",
      {
        "title": "Settings",
        "items": [
          "yuanqing.sketch-clean-document.settings.settings",
          "-",
          "yuanqing.sketch-clean-document.settings.reset-settings"
        ]
      }
    ]
  }
}
