{
  "name": "sketch-clean-document",
  "version": "0.0.2",
  "description": "A Sketch plugin to automagically organise and clean up your Sketch document",
  "keywords": [
    "plugin",
    "sketch",
    "sketch-plugin-helper"
  ],
  "license": "MIT",
  "author": "Lim Yuan Qing",
  "repository": "yuanqing/sketch-clean-document",
  "dependencies": {
    "sketch-sort-layer-list": "*"
  },
  "devDependencies": {
    "sketch-plugin-helper": "*"
  },
  "scripts": {
    "build": "sph build",
    "fix": "sph lint --fix",
    "lint": "sph lint",
    "symlink": "sph symlink",
    "unlink": "sph symlink --delete",
    "version": "sph version",
    "watch": "sph build --watch"
  },
  "sketch-plugin-helper": {
    "name": "Clean Document",
    "menu": [
      {
        "handler": "clean-document",
        "label": "Clean Document"
      },
      "-",
      {
        "handler": "clean-layers/clean-layers",
        "label": "Clean Layers"
      },
      {
        "handler": "clean-styles-and-symbols/clean-styles-and-symbols",
        "label": "Clean Styles and Symbols"
      },
      "-",
      {
        "Settings": [
          {
            "handler": "clean-layers/settings",
            "label": "Settings for Clean Layers"
          },
          {
            "handler": "clean-styles-and-symbols/settings",
            "label": "Settings for Clean Styles and Symbols"
          },
          "-",
          {
            "handler": "reset-settings",
            "label": "Reset Settings"
          }
        ]
      }
    ],
    "defaultSettings": {
      "cleanLayers.deleteHiddenLayers": true,
      "cleanLayers.smartRenameLayers": true,
      "cleanLayers.smartSortLayers": true,
      "cleanLayers.unnestNestedGroups": true,
      "cleanLayers.whitelistRegularExpression": "",
      "cleanStylesAndSymbols.deleteUnusedLayerStyles": true,
      "cleanStylesAndSymbols.deleteUnusedSymbols": true,
      "cleanStylesAndSymbols.deleteUnusedTextStyles": true
    }
  }
}
