{
  "name": "sketch-clean-document",
  "version": "0.0.2",
  "description": "A Sketch plugin to automagically organise and clean up your Sketch document",
  "keywords": [
    "plugin",
    "sketch",
    "sketch-plugin-helper"
  ],
  "license": "MIT",
  "author": "Lim Yuan Qing",
  "repository": "yuanqing/sketch-clean-document",
  "dependencies": {
    "natural-compare-lite": "^1.4.0",
    "sketch-sort-layer-list": "*"
  },
  "devDependencies": {
    "sketch-plugin-helper": "*"
  },
  "scripts": {
    "build": "sph build",
    "fix": "sph lint --fix",
    "lint": "sph lint",
    "symlink": "sph symlink",
    "unlink": "sph symlink --delete",
    "version": "sph version",
    "watch": "sph build --watch"
  },
  "sketch-plugin-helper": {
    "name": "Clean Document",
    "menu": [
      {
        "handler": "clean-document",
        "label": "Clean Document"
      },
      "-",
      {
        "handler": "clean-layers/clean-layers",
        "label": "Clean Layers"
      },
      {
        "handler": "clean-symbols/clean-symbols",
        "label": "Clean Symbols"
      },
      {
        "handler": "clean-styles/clean-styles",
        "label": "Clean Styles"
      },
      {
        "handler": "clean-pages/clean-pages",
        "label": "Clean Pages"
      },
      "-",
      {
        "Settings": [
          {
            "handler": "settings/settings",
            "label": "Settings"
          },
          "-",
          {
            "handler": "settings/reset-settings",
            "label": "Reset Settings"
          }
        ]
      }
    ],
    "defaultSettings": {
      "cleanLayers.deleteHiddenLayers": true,
      "cleanLayers.smartRenameLayers": true,
      "cleanLayers.smartSortLayers": true,
      "cleanLayers.unnestNestedGroups": true,
      "cleanLayers.whitelistRegularExpression": "",
      "cleanPages.deleteEmptyPages": true,
      "cleanPages.sortPages": true,
      "cleanStyles.deleteUnusedLayerStyles": true,
      "cleanStyles.deleteUnusedTextStyles": true,
      "cleanSymbols.deleteUnusedSymbols": true,
      "cleanSymbols.organiseSymbols": true,
      "cleanSymbols.groupDefinition": "2nd",
      "cleanSymbols.space": 100
    }
  }
}
